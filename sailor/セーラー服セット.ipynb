    "# 自分の得意な言語で\n",
    "# Let's チャレンジ！！\n",
    "num = int(input())\n",
    "\n",
    "words = [input() for n in range(num)]\n",
    "\n",
    "for i in range(len(words)):\n",
    "    if i != len(words) - 1:\n",
    "        print(words[i] + \"_\" , end = \"\")\n",
    "    else:\n",
    "        print(words[i])"
